FILTER[vega20]=\
"$BLACKLIST_ALL_ASICS:"\
"$SDMA_BLACKLIST:"\
"KFDEvictTest.BurstyTest:"\
"KFDQMTest.GPUDoorbellWrite"

FILTER[raven]=\
"$BLACKLIST_ALL_ASICS:"\
"KFDQMTest.QueueLatency:"\
